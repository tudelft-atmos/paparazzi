<!DOCTYPE settings SYSTEM "../settings.dtd">
<settings>
  <dl_settings>
    <dl_settings NAME="multiGain">
      <dl_setting var="activeGainSet" min="1" step="1" max="2" module="modules/transitioning_vehicles/multiGain" shortname="gainSet" values="A|B" handler="SetGainSetHandler"/>       
    </dl_settings>
    <dl_settings NAME="[A]Att Loop">
      <dl_setting var="stabilization_gainsA.p.x" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain phi" />
      <dl_setting var="stabilization_gainsA.d.x" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain p"/>
      <dl_setting var="stabilization_gainsA.i.x" min="0"  step="1" max="50"    module="modules/transitioning_vehicles/multiGain" shortname="igain phi" handler="SetKiPhi"/>
      <dl_setting var="stabilization_gainsA.dd.x" min="0"    step="1" max="500" module="modules/transitioning_vehicles/multiGain" shortname="ddgain p"/>
   
      <dl_setting var="stabilization_gainsA.p.y" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain theta"/>
      <dl_setting var="stabilization_gainsA.d.y" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain q"/>
      <dl_setting var="stabilization_gainsA.i.y" min="0"  step="1" max="60"    module="modules/transitioning_vehicles/multiGain" shortname="igain theta"/>
      <dl_setting var="stabilization_gainsA.dd.y" min="0"    step="1" max="500"  module="modules/transitioning_vehicles/multiGain" shortname="ddgain q"/>

      <dl_setting var="stabilization_gainsA.p.z" min="0" step="1" max="8000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain psi"/>
      <dl_setting var="stabilization_gainsA.d.z" min="0" step="1" max="8000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain r"/>
      <dl_setting var="stabilization_gainsA.i.z" min="0"  step="1" max="80"    module="modules/transitioning_vehicles/multiGain" shortname="igain psi"/>
      <dl_setting var="stabilization_gainsA.dd.z" min="0"    step="1" max="500"  module="modules/transitioning_vehicles/multiGain" shortname="ddgain r"/>
    </dl_settings>
    <dl_settings NAME="[B]Att Loop">
      <dl_setting var="stabilization_gainsB.p.x" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain phi" />
      <dl_setting var="stabilization_gainsB.d.x" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain p"/>
      <dl_setting var="stabilization_gainsB.i.x" min="0"  step="1" max="50"    module="modules/transitioning_vehicles/multiGain" shortname="igain phi" handler="SetKiPhi"/>
      <dl_setting var="stabilization_gainsB.dd.x" min="0"    step="1" max="500" module="modules/transitioning_vehicles/multiGain" shortname="ddgain p"/>
   
      <dl_setting var="stabilization_gainsB.p.y" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain theta"/>
      <dl_setting var="stabilization_gainsB.d.y" min="0" step="1" max="6000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain q"/>
      <dl_setting var="stabilization_gainsB.i.y" min="0"  step="1" max="60"    module="modules/transitioning_vehicles/multiGain" shortname="igain theta"/>
      <dl_setting var="stabilization_gainsB.dd.y" min="0"    step="1" max="500"  module="modules/transitioning_vehicles/multiGain" shortname="ddgain q"/>

      <dl_setting var="stabilization_gainsB.p.z" min="0" step="1" max="8000"   module="modules/transitioning_vehicles/multiGain" shortname="pgain psi"/>
      <dl_setting var="stabilization_gainsB.d.z" min="0" step="1" max="8000"   module="modules/transitioning_vehicles/multiGain" shortname="dgain r"/>
      <dl_setting var="stabilization_gainsB.i.z" min="0"  step="1" max="80"    module="modules/transitioning_vehicles/multiGain" shortname="igain psi"/>
      <dl_setting var="stabilization_gainsB.dd.z" min="0"    step="1" max="500"  module="modules/transitioning_vehicles/multiGain" shortname="ddgain r"/>
    </dl_settings>
  </dl_settings>
</settings>
